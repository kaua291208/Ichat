{
    "expo": {
      "name": "iChat",
      "slug": "ichat-mobile",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "userInterfaceStyle": "light",
      "newArchEnabled": true,
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#007AFF"
      },
      "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.ichat.mobile"
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/adaptive-icon.png",
          "backgroundColor": "#007AFF"
        },
        "permissions": [
          "android.permission.POST_NOTIFICATIONS"
        ],
        "package": "com.ichat.mobile"
      },
      "plugins": [
        [
          "expo-notifications",
          {
            "icon": "./assets/notification-icon.png",
            "color": "#007AFF"
          }
        ]
      ]
    }
  }